{
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
   "rules": {
    "linebreak-style": ["error", "windows"],
    "comma-dangle": 0,
    "object-curly-spacing": 0,
    "arrow-body-style": 0,
    "no-else-return": 0,
    "arrow-parens": 0
  },
  "parser": "babel-eslint",
  "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
      "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "jest": true
    },
    "globals": {
      "DEBUG": false
  }
 
  
}